<div class="container">
  <div class="well">
    <h2>Sign up</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="form-group">
        <%= f.label :First_Name %><br />
        <%= f.text_field :first_name, autofocus: true, class: "form-control", placeholder: "Umesh" %>
      </div>
      
      <div class="form-group">
        <%= f.label :Last_Name %><br />
        <%= f.text_field :last_name, autofocus: true, class: "form-control", placeholder: "Kafle" %>
      </div>

      <div class="form-group">
        <%= f.label :Birthday %><br />
        <%= f.text_field :dob, autofocus: true, class: "form-control", placeholder: "13/07/1992" %>
      </div>

      <div class="form-group">
        <%= f.label :Address %><br />
        <%= f.text_area :address, autofocus: true, class: "form-control", placeholder: "67, kingsley road, south harrow" %>
      </div>

      <div class="form-group">
        <%= f.label :Phone_No %><br />
        <%= f.text_field :phone_no, autofocus: true, class: "form-control", placeholder: "07853150254" %>
      </div>

      <div class="form-group">
        <%= f.label :Username %><br />
        <%= f.text_field :username, autofocus: true, class: "form-control", placeholder: "UmeshKafle" %>
      </div>

      <div class="form-group">
        <%= f.label :CollegeName %><br />
        <%= f.email_field :college_name, autofocus: true, class: "form-control", placeholder: "Islington College" %>
      </div>

      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "umesh@example.com" %>
      </div>

      <div class="form-group">
        <%= f.label :password %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "password" %>
      </div>

      <div class="form-group">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control", placeholder: "Re-type your password" %>
      </div>

      <div class="actions">
        <%= f.submit "Sign up", class: "btn btn-info" %>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>